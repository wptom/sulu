{% extends 'base.html.twig' %}

{% block content %}
    <h2>Showing {{ hits|length }} results for "{{ query }}"</h2>

    <ul>
        {% for i, hit in hits %}
            <li>
                <a href="{{ sulu_content_path(hit.url) }}">
                    {{- hit.title -}}
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
